{
  "name": "eslint-plugin-chalk",
  "version": "1.0.0",
  "description": "ESLint plugin that always reports with configurable message",
  "main": "dist/index.js",
  "author": "Xuqing Kuang",
  "license": "MIT",
  "homepage": "https://github.com/xuqingkuang/eslint-plugin-chalk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuqingkuang/eslint-plugin-chalk.git"
  },
  "type": "commonjs",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts ./ && tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "ci": "run-s \"ci:*\"",
    "ci:lint": "eslint ./",
    "ci:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "eslint": ">=1.0.0"
  }
}
